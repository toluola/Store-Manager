{
  "name": "store-manager",
  "version": "1.0.0",
  "description": "Store managing app",
  "main": "server/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- server/index.js",
    "test": "nyc mocha server/tests/ --require babel-core/register -t 5000 --exit",
    "test:watch": "nodemon --exec 'npm test'",
    "lint": "eslint --ext .js server",
    "lint:watch": "nodemon --exec 'eslint --ext .js server'",
    "lint:fix": "eslint --ext .js server --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel --ignore ./server/tests -d ./dist ./server -s",
    "clean": "rm -rf dist && mkdir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toluola/Store-Manager.git"
  },
  "author": "Tolu' Olaniyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toluola/Store-Manager/issues"
  },
  "homepage": "https://github.com/toluola/Store-Manager#readme",
  "nyc": {
    "exclude": ["UI/**.js", "**/**.spec.js"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.2",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0",
    "underscore": "^1.9.1"
  }
}
